.release .filter-bg {
  position: fixed;
  left: 0;
  top: 0;
  width: 100vw;
  height: 100vh;
  background-image: url(data:image/jpeg;base64,);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-attachment: fixed;
  overflow: hidden;
  -webkit-filter: blur(80rpx);
          filter: blur(80rpx);
  opacity: 0.4;
}
.release .content {
  position: absolute;
  left: 0;
  top: 0;
  width: 100vw;
  height: 90vh;
}
.release .content .fonts {
  padding: 0 110rpx;
  margin-top: 50rpx;
}
.release .content .fonts text {
  display: block;
  font-size: 50rpx;
  padding: 5rpx;
}
.release .content .line {
  width: 100rpx;
  border-top: 3rpx solid #555;
  margin-left: 110rpx;
}
.release .content .type {
  width: 700rpx;
  position: absolute;
  height: 380rpx;
  bottom: 200rpx;
  z-index: 3;
  display: flex;
  flex-wrap: wrap;
}
.release .content .type > view {
  height: 180rpx;
  width: 110rpx;
  margin-left: 108rpx;
  font-size: 26rpx;
  font-family: Microsoft YaHei;
  font-weight: 400;
  color: #333;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.release .content .type > view > image {
  width: 110rpx;
  height: 110rpx;
}
.release .content .type > view > text {
  margin-top: 10rpx;
}
.release .content .del {
  width: 100vw;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  position: fixed;
  bottom: 30rpx;
  z-index: 2;
}
.release .content .del image {
  width: 28rpx;
  height: 28rpx;
}

